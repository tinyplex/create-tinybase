{
  "name": "{{projectName}}",
  "version": "1.0.0",
  "scripts": {
{{#list}}
    "dev": "vite"
{{#if isTypescript}}
    "build": "tsc && vite build"
{{else}}
    "build": "vite build"
{{/if}}
    "preview": "vite preview"
{{#if prettier}}
    "format": "prettier --write ."
    "format:check": "prettier --check ."
{{/if}}
{{#if eslint}}
    "lint": "eslint ."
{{/if}}
{{/list}}
  },
  "devDependencies": {
{{#list}}
    "vite": "^7.1.3"
{{#if isTypescript}}
    "typescript": "^5.9.3"
    "@types/node": "^25.0.3"
{{#if isReact}}
    "@types/react": "^18.3.18"
    "@types/react-dom": "^18.3.5"
{{/if}}
{{/if}}
{{#if isReact}}
    "@vitejs/plugin-react": "^4.3.4"
{{/if}}
{{#if prettier}}
    "prettier": "^3.7.4"
{{/if}}
{{#if eslint}}
    "eslint": "^9.39.2"
{{#if isTypescript}}
    "typescript-eslint": "^8.51.0"
{{/if}}
{{#if isReact}}
    "eslint-plugin-react": "^7.37.3"
    "eslint-plugin-react-hooks": "^5.1.0"
{{/if}}
{{/if}}
{{/list}}
  },
  "dependencies": {
{{#list}}
    "tinybase": "^6.6.0"
{{#if isReact}}
    "react": "^18.3.1"
    "react-dom": "^18.3.1"
{{/if}}
{{/list}}
  }
}