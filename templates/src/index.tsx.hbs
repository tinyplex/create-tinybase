{{#if react}}
  import ReactDOM from 'react-dom/client';
  import {App} from './{{appType}}/App';
  {{includeFile template="src/{{appType}}/App.tsx.hbs" output="src/{{appType}}/App.{{ext}}"}}

  addEventListener('load', () =>
  ReactDOM.createRoot(document.getElementById('app')!).render(
  <App />),
  );
{{else}}
  import {app} from './{{appType}}/app';
  {{includeFile template="src/{{appType}}/app.ts.hbs" output="src/{{appType}}/app.{{ext}}"}}

  addEventListener('load', app);
{{/if}}