{{#if react}}
  import ReactDOM from 'react-dom/client';
  import {App} from './App';
  {{includeFile template="src/{{appType}}/App.tsx.hbs" output="src/App.{{ext}}"}}

  addEventListener('load', () =>
  ReactDOM.createRoot(document.getElementById('app')!).render(
  <App />),
  );
{{else}}
  import {app} from './app';
  {{includeFile template="src/{{appType}}/app.ts.hbs" output="src/app.{{ext}}"}}

  addEventListener('load', app);
{{/if}}